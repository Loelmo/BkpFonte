ALTER PROCEDURE [dbo].[STP_SelecionaAdmGrupo]
(
 @nCEA_PROGRAMA INT
)
As    
BEGIN     

SET NOCOUNT ON  

  SELECT
		CDA_GRUPO,
		TX_GRUPO,
		TX_DESCRICAO,
		CEA_PROGRAMA
   FROM TBL_ADM_GRUPO WHERE CEA_PROGRAMA = @nCEA_PROGRAMA;
  
RETURN  

END  
    
