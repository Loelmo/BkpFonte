ALTER PROCEDURE [dbo].[STP_SelecionaAdmGrupoPorId]
(
 @nIdAdmGrupo int
)
As    
BEGIN     

SET NOCOUNT ON  

  SELECT
		CDA_GRUPO,
		TX_GRUPO,
		TX_DESCRICAO,
		CEA_PROGRAMA
   FROM TBL_ADM_GRUPO
  WHERE CDA_GRUPO = @nIdAdmGrupo
  
RETURN  

END  
    
