using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Vinit.SG.Ent
{
    public class EntInscricoesEmpresa
    {
        private EntTurmaEmpresa _TurmaEmpresa;
        public EntTurmaEmpresa TurmaEmpresa
        {
            get
            {
                if (_TurmaEmpresa == null)
                {
                    _TurmaEmpresa = new EntTurmaEmpresa();
                }
                return _TurmaEmpresa;
            }

            set { _TurmaEmpresa = value; }
        }

        private EntProgramaEmpresa _ProgramaEmpresa;
        public EntProgramaEmpresa ProgramaEmpresa
        {
            get
            {
                if (_ProgramaEmpresa == null)
                {
                    _ProgramaEmpresa = new EntProgramaEmpresa();
                }
                return _ProgramaEmpresa;
            }

            set { _ProgramaEmpresa = value; }
        }
    }
}
