using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Vinit.SG.Ent
{
    /// <summary>
    /// Classe de entidade que representa um Programa
    /// </summary>
    [Serializable()]
    public class EntPrograma
    {
        public const int PROGRAMA_MPE = 1;
        public const int PROGRAMA_FGA = 2;
        public const int PROGRAMA_PEG = 3;

        public Int32 IdPrograma { get; set; }

        public String Programa { get; set; }

        public Boolean Ativo { get; set; }

        public Boolean Publico { get; set; }

        public Boolean Publicado { get; set; }

        public DateTime DtCadastro { get; set; }
    }
}
